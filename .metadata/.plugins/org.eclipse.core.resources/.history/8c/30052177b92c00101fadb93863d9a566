public class Eventos_1 {
	
		public static void main(String[] args) {
			
			MarcoBotones mimarco=new MarcoBotones();
			
			mimarco.setVisible(true);
			
			mimarco.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
			
	}
}

